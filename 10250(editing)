#include <stdio.h>

int main(void) 
{
	int t;
	int w, h, n;
	int height, num;
	
	scanf("%d", &n);
	
	for(int i = 0; i < t; i++)
	{
		scanf("%d%d%d", w, h, n);
		
		if(n % h == 0)
		{
			num = n / h;
			height = h;
		}
		else
		{
			num = n / h + 1;
			height = n % h;
		}
		
		printf("%d%c%d\n", height, (num / 10 == 0) ? '0' : '', num);
	}
	
	return 0;
}
