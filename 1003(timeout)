#include <stdio.h>

int count_0;
int count_1;

int fibonacci(int k);

int main(void) 
{
	int n;
	int k;
	
	scanf("%d", &n);
	
	for(int i = 0; i < n; i++)
	{
		count_0 = 0;
		count_1 = 0;
		scanf("%d", &k);
		fibonacci(k);
		printf("%d %d\n", count_0, count_1);
	}
	return 0;
}

int fibonacci(int k)
{
	if(k == 0)
	{
		count_0++;
		return 0;
	}
	else if(k == 1)
	{
		count_1++;
		return 1;
	}
	else
	{
		return fibonacci(k-1) + fibonacci(k-2);
	}
}
