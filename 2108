#include <stdio.h>

#define INPUT_SIZE 8001
#define CORR_NUM 4000

double calc_mean(int*, int);
int calc_median(int*, int);
int calc_mode(int*);
int calc_range(int*);

int main()
{
    int n;
    int temp;
    int ary[INPUT_SIZE] = { 0, };

    scanf("%d", &n);
    for(int i = 0; i < n; i++)
    {
        scanf("%d", &temp);
        ary[temp + CORR_NUM]++;
    }

    printf("%.lf\n", calc_mean(ary, n));
    printf("%d\n", calc_median(ary, n));
    printf("%d\n", calc_mode(ary));
    printf("%d\n", calc_range(ary));

    return 0;
}

double calc_mean(int* ary, int n)
{
    double mean;
    double sum = 0;

    for(int i = 0; i < INPUT_SIZE; i++)
    {
        sum += (i - CORR_NUM) * ary[i];
    }

    mean = sum / (double)n;

    return mean;
}

int calc_median(int* ary, int n)
{
    int median;
    int count = 0;

    for(int i = 0; i < INPUT_SIZE; i++)
    {
        count += ary[i];
        if(count > n / 2)
        {
            median = i - CORR_NUM;
            return median;
        }
    }
}

int calc_mode(int* ary)
{
    int mode = 0;

    for(int i = 0; i < INPUT_SIZE; i++)
    {
        if(ary[i] > ary[mode + CORR_NUM])
        {
            mode = i - CORR_NUM;
        }
        else if(ary[i] == ary[mode + CORR_NUM] && ary[i])
        {
            mode = i - CORR_NUM;
            break;
        }
    }

    return mode;
}

int calc_range(int* ary)
{
    int range;
    int min_num;
    int max_num;

//최소값
    for(int i = 0; i < INPUT_SIZE; i++)
    {
        if(ary[i])
        {
            min_num = i - CORR_NUM;
            break;
        }
    }

//최대값
    for(int i = INPUT_SIZE - 1; i > 0; i--)
    {
        if(ary[i])
        {
            max_num = i - CORR_NUM;
            break;
        }
    }

    range = max_num - min_num;

    return range;
}
